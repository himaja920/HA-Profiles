Device* getDevice(int devid) {
    list <Device>::iterator iterator;
  char buffer[123556];
    for (iterator = dL.devices.begin(); iterator != dL.devices.end(); ++iterator) {
        if ((*iterator).getID() == devid) {
           
            break;//swprintf(opt,100,L"%s",text);

        }
    }
    if (iterator == dL.devices.end())
        return NULL;
    else
        return &(*iterator);
}

