float getTempinFloat(char *str) {
    int i = 0;
    char tbuff[10] = "";
    while (str[i] && (str[i] >= '0' && str[i] <= '9') || str[i] == '.') {
        tbuff[i] = str[i++];
    }
    return atof(tbuff);
}

